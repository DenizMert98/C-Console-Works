namespace Ortalama_Geçti_Kaldı
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Sınav1 notunuzu giriniz");
            int s1 = int.Parse(Console.ReadLine());
            Console.WriteLine("Sınav2 notunuzu giriniz");
            int s2 = int.Parse(Console.ReadLine());
            Console.WriteLine("Proje notunuzu giriniz");
            int proje = int.Parse(Console.ReadLine());

            double ortalama = (s1 * 0.2) + (s2 * 0.3) + (proje * 0.5);

            if (ortalama >= 60)
            {
                Console.WriteLine("Ortalamanız " + ortalama + " GEÇTİNİZ");
            }
            else
            {
                Console.WriteLine("Ortalamanız " + ortalama + " KALDINIZ");
            }
            Console.Read();
        }
    }
}
